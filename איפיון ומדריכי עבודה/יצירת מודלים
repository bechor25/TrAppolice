npx sequelize-cli model:generate --name citizen --attributes Last_Name:string,First_Name:string,ID_Number:integer

npx sequelize-cli model:generate --name Cop --attributes Cop_ID:integer,Password:string,First_Name:string,Last_Name:string,Rank:string

npx sequelize-cli model:generate --name Police_Station_Server --attributes Citizen_ID:integer,Citizen_FullName:string

npx sequelize-cli model:generate --name Traffic_Law --attributes Search_By_Word:string,Points:integer,Offense_Number:integer

